{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prebuild": {},
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "test:ci": {
      "outputs": ["coverage/**"]
    },
    "test:": {
      "outputs": ["coverage/**"]
    },
    "lint:": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
