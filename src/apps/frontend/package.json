{
  "name": "@iamnick/frontend",
  "version": "1.0.0",
  "private": true,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/derozn/iamnick.dev.git"
  },
  "author": "iamnick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/derozn/iamnick.dev/issues"
  },
  "homepage": "https://github.com/derozn/iamnick.dev#readme",
  "scripts": {
    "prebuild": "rimraf build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --config=./test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --ci --maxWorkers=3 --coverage --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "lint": "prettier --check --ignore-path ../../../.prettierignore . && eslint --cache .",
    "lint:fix": "prettier --write --ignore-path ../../../.prettierignore . && eslint --cache --fix .",
    "postbuild": "next-sitemap --config next-sitemap.config.js"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@derozn/tailwind-variants": "0.2.6",
    "@iamnick/ui": "*",
    "@react-three/drei": "9.111.2",
    "@react-three/fiber": "8.17.5",
    "@react-three/postprocessing": "2.16.2",
    "framer-motion": "11.3.29",
    "intersection-observer": "0.12.2",
    "next": "14.2.5",
    "next-seo": "6.5.0 ",
    "next-sitemap": "4.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "three": "0.167.1"
  },
  "devDependencies": {
    "@banterstudiosuk/eslint-config-typescript": "0.2.5",
    "@banterstudiosuk/prettier-config": "0.2.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@iamnick/config": "*",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/css-mediaquery": "0.1.4",
    "@types/jest": "29.5.12",
    "@types/node": "22.4.2",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@types/three": "0.167.1",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "@xpd/tailwind-3dtransforms": "1.0.3",
    "autoprefixer": "10.4.20",
    "classnames": "2.4.0",
    "css-mediaquery": "0.1.2",
    "eslint": "9.9.0",
    "eslint-import-resolver-typescript": "3.6.1",
