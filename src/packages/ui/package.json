{
  "name": "@iamnick/ui",
  "version": "1.0.0",
  "description": "iamnick Design System",
  "private": true,
  "author": "iamnick",
  "license": "ISC",
  "homepage": "https://github.com/derozn/iamnick.dev#readme",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "imports": {
    "@iamnick/ui/*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/derozn/iamnick.dev.git"
  },
  "files": [
    "*",
    "!**/*.test.*",
    "!coverage",
    "!.turbo",
    "!.vscode",
    "!node_modules",
    "!reports"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc -p 'tsconfig.json'",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "jest --ci --maxWorkers=3 --coverage --passWithNoTests",
    "lint": "prettier --check --ignore-path ../../../.prettierignore . && eslint --cache .",
    "lint:fix": "prettier --write --ignore-path ../../../.prettierignore . && eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "yarn build-icons && storybook build",
    "build-icons": "svgr --typescript --jsx-runtime automatic --out-dir src/components/atoms/Icons --svgo-config .svgo.json --title-prop --no-dimensions --ignore-existing -- src/assets/icons"
  },
  "peerDependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-react": "7.24.7",
    "@banterstudiosuk/eslint-config-typescript": "0.2.5",
    "@banterstudiosuk/prettier-config": "0.2.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@mdx-js/react": "1.6.22",
    "@newhighsco/storybook-addon-transpile-modules": "2.0.24",
    "@storybook/addon-actions": " 8.2.9",
    "@storybook/addon-docs": "8.2.9",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-mdx-gfm": "8.2.9",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/cli": "8.2.9",
    "@storybook/nextjs": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/testing-library": "0.2.2",
    "@svgr/cli": "8.1.0",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/lodash": "4.17.7",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "@xpd/tailwind-3dtransforms": "1.0.3",
    "autoprefixer": "10.4.20",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "eslint": "9.9.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.8.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-storybook": "0.6.13",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jsdom": "24.1.1",
    "next": "14.2.5",
    "postcss": "8.4.41",
    "postcss-import": "16.1.0",
    "prettier": "3.3.3",
    "rimraf": "5.0.1",
    "storybook": "8.2.9",
    "tailwindcss": "3.4.10",
    "ts-jest": "29.2.4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@derozn/tailwind-variants": "0.2.6",
    "@emotion/is-prop-valid": "1.3.0",
    "@iamnick/config": "*",
    "classnames": "2.4.0",
    "framer-motion": "11.3.29",
    "lodash": "4.17.21",
    "react-loader-spinner": "5.4.5",
    "react-loading-skeleton": "3.4.0",
    "tailwindcss": "3.3.3"
  }
}
